    private void LoginButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_LoginButtonActionPerformed
        // TODO add your handling code here:
        if (UsserField.getText().equals("AlvinAryan")){ 
            if (PassField.getText().equals("pulupulu")){
               new Data_penjualanhp().show();
               dispose();
               }
            else
               JOptionPane.showMessageDialog(null,"Passwordnya Salah Bro");
               }
            else
                JOptionPane.showMessageDialog(null,"Siapa lu? kok Salah?");
    
    }//GEN-LAST:event_LoginButtonActionPerformed

